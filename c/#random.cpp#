#include <iostream>

using std::cin; using std::cout; using std::endl;

int twice(int *t){

  cout << t << endl;
  cout << &t << endl;
  cout << *t << endl; // pekade på adress -> adress med tal -> tal 
  
}


int main() {

  int f = 123; // VarAdress -> tal
  cout << &f << endl;
  int * r = &f; //VarAdress -> pekade på adress
  twice(r);
  return 0;

}
